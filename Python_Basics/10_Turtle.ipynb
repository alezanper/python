    "# Using turtle \n",
    "import turtle\n",
    "\n",
    "y = 5\n",
    "for i in range(y):\n",
    "    turtle.forward(100)\n",
    "    turtle.right(360/y)\n",
    "\n",
    "##Nested\n",
    "for i in range(y) :\n",
    "    turtle.forward(100)\n",
    "    turtle.right(360/y)\n",
    "    for j in range(y):\n",
    "        turtle.forward(50)\n",
    "        turtle.right(360/y)\n",
    "turtle.\n",
